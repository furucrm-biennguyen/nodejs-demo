{
  "name": "nodejs-demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "run:dev": "ts-node --transpile-only -r tsconfig-paths/register ./src/Bootstrap.ts",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "test:repository": "jest ./Repositories/*.spec.ts --watch",
    "test:service": "jest ./Services/*.spec.ts --watch",
    "test:controller": "jest ./Controllers/*.spec.ts --watch",
    "test:final": "jest ./Bootstrap.spec.ts --watch",
    "migrate": "node-pg-migrate"
  },
  "dependencies": {
    "@types/morgan": "^1.9.4",
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "express-validator": "^6.15.0",
    "inversify": "^6.0.1",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.9",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.30.0",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "concurrently": "^8.0.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "inversify-express-utils": "^6.4.3",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.3"
  }
}
